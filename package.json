{
  "name": "essay",
  "version": "4.0.0",
  "description": "Single User Blogging System",
  "private": false,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "@journeyapps/sqlcipher": "^5.3.1",
    "cherry-markdown": "^0.8.20",
    "egg": "^3",
    "egg-orm": "^2.3.0",
    "egg-scripts": "^2",
    "egg-view-ejs": "^3.0.0",
    "md5": "^2.3.0",
    "sqlite3": "^5.1.6",
    "uid": "^2.0.2"
  },
  "devDependencies": {
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "browserslist": "last 2 versions",
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-EssayCMS",
    "stop": "egg-scripts stop --title=egg-server-EssayCMS",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov"
  },
  "ci": {
    "version": "16, 18",
    "type": "github"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "wmui",
  "license": "MIT"
}
